#!/usr/bin/env bash
docker run -d -p 8000:8000 \
    -e MYSQL_NAME=minimart \
    -e MYSQL_USER=minimart \
    -e MYSQL_PASSWORD=minimart \
    -e MYSQL_HOST=127.0.0.1 \
    -e MYSQL_PORT=3306 \
    --restart on-failure:5 \
    --name minimart-server \
    --network="host" \
    tranvannhan1911/minimart-server:v106